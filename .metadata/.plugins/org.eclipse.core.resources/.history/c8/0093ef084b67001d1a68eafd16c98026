package fundamentos;

public class Wrappers {
	public static void main(String[] args) {
		
		//byte
		Byte b = 22;
		Short s = 1000;
		Integer i = Integer.parseInt("1000"); //int
		Long l = 100000L;
		
		System.out.println(b.byteValue());
		System.out.println(s.toString());
		System.out.println(i);
	}
}
